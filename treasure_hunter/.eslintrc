{
    "extends": [
        "plugin:vue/recommended",
        "eslint-config-airbnb"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-new": "off"
    }
}